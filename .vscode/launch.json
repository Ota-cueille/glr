	"version": "0.2.0",
	"configurations": [
		{
			"name": "debug-windows",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/build/debug/a.exe",
			"cwd": "${workspaceFolder}",
			"terminal": "integrated",
			"preLaunchTask": "windows-build-debug"
		},
		{
			"name": "release-windows",
			"type": "lldb",
			"request": "launch",
			"program": "${workspaceFolder}/build/release/a.exe",
			"cwd": "${workspaceFolder}",
			"terminal": "integrated",
			"preLaunchTask": "windows-build-release"
		},
	]
}